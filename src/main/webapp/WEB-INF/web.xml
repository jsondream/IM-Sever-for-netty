<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns:web="http://xmlns.jcp.org/xml/ns/javaee">
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>WebhooksCallBack</servlet-name>
		<servlet-class>com.jsondream.pingpp.WebhooksCallBack</servlet-class>
	</servlet>
	<!-- ... -->
	<servlet-mapping>
		<servlet-name>WebhooksCallBack</servlet-name>
		<url-pattern>/pingpp/webhooks</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChargeServlet</servlet-name>
